{
  "manifest_version": 2,
  "name": "DJs Everwhere",
  "short_name": "DJs Everywhere",
  "version": "0.1",
  "default_locale": "en",
  "description": "It turns out everyone is a DJ",
  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}

